{
  "name": "psy-app-server",
  "version": "1.0.0",
  "description": "A API for an psy-app",
  "main": "/src/server.ts",
  "type": "module",
  "scripts": {
    "dev": "tsx --watch src/server.ts",
    "migrate:list": "node --import tsx ./node_modules/.bin/knex migrate:list",
    "migrate:make": "node --import tsx ./node_modules/.bin/knex migrate:make",
    "migrate:latest": "node --import tsx ./node_modules/.bin/knex migrate:latest",
    "migrate:down": "node --import tsx ./node_modules/.bin/knex migrate:down",
    "migrate:rollback": "node --import tsx ./node_modules/.bin/knex migrate:rollback --all",
    "seed:make": "node --import tsx ./node_modules/.bin/knex seed:make",
    "seed:run": "node --import tsx ./node_modules/.bin/knex seed:run"
  },
  "author": "Dev-Nadson",
  "license": "ISC",
  "dependencies": {
    "@paralleldrive/cuid2": "^3.1.0",
    "bcryptjs": "^3.0.2",
    "dotenv": "^17.2.3",
    "fastify": "^5.6.1",
    "knex": "^3.1.0",
    "pg": "^8.16.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^24.7.2",
    "tsx": "^4.20.6"
  }
}
